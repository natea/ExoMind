{
  "name": "exomind-life-os",
  "version": "1.0.0",
  "description": "Life OS Skills implementation for ExoMind - Personal productivity and life management system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:skills": "jest tests/skills",
    "test:templates": "jest tests/templates",
    "test:config": "jest tests/config",
    "test:integration": "jest tests/integration",
    "test:todoist": "jest tests/integrations/todoist --config tests/integrations/todoist/jest.config.js",
    "test:todoist:watch": "jest tests/integrations/todoist --watch",
    "test:todoist:coverage": "jest tests/integrations/todoist --coverage --config tests/integrations/todoist/jest.config.js",
    "test:todoist:e2e": "jest tests/integration/todoist-full-cycle.test.ts",
    "test:google": "jest tests/integrations/gmail tests/integrations/calendar tests/integration/google-workspace-flow.test.ts",
    "test:google:watch": "jest tests/integrations/gmail tests/integrations/calendar --watch",
    "test:google:coverage": "jest tests/integrations/gmail tests/integrations/calendar tests/integration/google-workspace-flow.test.ts --coverage",
    "validate-structure": "ts-node scripts/validate-structure.ts",
    "validate-structure:repair": "ts-node scripts/validate-structure.ts --repair",
    "validate-templates": "ts-node scripts/validate-templates.ts",
    "validate-templates:repair": "ts-node scripts/validate-templates.ts --repair",
    "validate:all": "ts-node scripts/validate-production-readiness.ts",
    "validate:production": "ts-node scripts/validate-production-readiness.ts",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "life-os",
    "productivity",
    "personal-management",
    "gtd",
    "planning",
    "goals"
  ],
  "author": "ExoMind Team",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
